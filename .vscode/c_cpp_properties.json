{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "~/Arduino/libraries/**",
                "~/.arduino15/packages/arduino/tools/**",
                "~/.arduino15/packages/arduino/hardware/avr/1.8.5/",
                "~/.arduino15/packages/arduino/hardware/avr/1.8.5/cores/arduino/**",
                "~/.arduino15/packages/arduino/hardware/avr/1.8.5/firmwares/**",
                "~/.arduino15/packages/arduino/hardware/avr/1.8.5/libraries/**",
                /* ↓↓↓↓ Change this if your Arduino's architecture is not "mega" ↓↓↓↓ */
                "~/.arduino15/packages/arduino/hardware/avr/1.8.5/variants/mega/**"
            ],
            "forcedInclude": [
                "~/.arduino15/packages/arduino/hardware/avr/1.8.5/cores/arduino/Arduino.h"
            ],
            "defines": [
                "USBCON",
                "DEBUG_MODE",
                "__AVR_ATmega2560__",
                "__AVR_ATmega32U4__"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-arm"
        }
    ],
    "version": 4
}
