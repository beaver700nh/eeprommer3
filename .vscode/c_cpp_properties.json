{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "~/Arduino/libraries/**",
                "~/.arduino15/packages/arduino/tools/**",
                "~/.arduino15/packages/arduino/hardware/avr/1.8.4/",
                "~/.arduino15/packages/arduino/hardware/avr/1.8.4/cores/arduino/**",
                "~/.arduino15/packages/arduino/hardware/avr/1.8.4/firmwares/**",
                "~/.arduino15/packages/arduino/hardware/avr/1.8.4/libraries/**",
                "~/.arduino15/packages/arduino/hardware/avr/1.8.4/variants/**"
            ],
            "forcedInclude": [
                "~/.arduino15/packages/arduino/hardware/avr/1.8.4/cores/arduino/Arduino.h",
                "~/.arduino15/packages/arduino/hardware/avr/1.8.4/variants/mega/pins_arduino.h"
            ],
            "defines": [
                "USBCON",
                "DEBUG_MODE"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-arm"
        }
    ],
    "version": 4
}
